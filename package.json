  "bugs": {
    "url": "https://github.com/Alpha-Atom/js-query-string/issues"
  },
  "homepage": "https://github.com/Alpha-Atom/js-query-string#readme",
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "coveralls": "^3.0.2",
    "istanbul": "^0.4.3",
    "jasmine": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jshint": "^2.9.1",
    "uglify-js": "^3.4.6"
  }
